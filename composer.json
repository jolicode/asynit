{
    "name": "jwurtz/asynit",
    "description": "Asynchronous HTTP Request Testing Library for API or more...",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Joel Wurtz",
            "email": "joel.wurtz@gmail.com"
        }
    ],
    "bin": [
        "bin/asynit"
    ],
    "autoload": {
        "psr-4": {
            "Asynit\\": "src/"
        }
    },
    "require": {
        "php-http/react-adapter": "^0.3",
        "php-http/message": "^1.3",
        "symfony/console": "^3.1",
        "beberlei/assert": "^2.6",
        "php-http/client-common": "^1.2",
        "doctrine/collections": "^1.3",
        "symfony/finder": "^3.1",
        "doctrine/annotations": "^1.2",
        "mkraemer/react-pcntl": "^2.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.6"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
